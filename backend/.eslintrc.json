{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "indent": [
            "error",
            "tab"
        ],
        "linebreak-style": "off",
        "quotes": [
            "error",
            "double"
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-tabs": "off",
        "no-console":"off",
        "no-unused-vars":"warn",
        "no-param-reassign":"off",
        "no-plusplus":"off",
        "radix":"off",
        "max-len": [
            "error",
            {
                "code": 90,
                "ignoreStrings": true,
                "ignoreComments": true,
                "ignoreTrailingComments": true
            }
        ],
        "import/extensions": "off"
    }
}
